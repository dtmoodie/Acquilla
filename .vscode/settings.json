{
    "files.associations": {
        "iosfwd": "cpp",
        "cmath": "cpp",
        "complex": "cpp",
        "cstdlib": "cpp",
        "system_error": "cpp",
        "*.ipp": "cpp"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "googletest.failed",
                "settings": {
                    "foreground": "#f00"
                }
            },
            {
                "scope": "googletest.passed",
                "settings": {
                    "foreground": "#0f0"
                }
            },
            {
                "scope": "googletest.run",
                "settings": {
                    "foreground": "#0f0"
                }
            }
        ]
    },
    "gtest-adapter.debugConfig": [
        "object",
        "core",
        "params",
        "cuda",
        "runtime_reflection",
        "serialization",
        "small_vec"
    ],
    "gtest-adapter.supportLocation": true,
    "C_Cpp.default.configurationProvider": "vector-of-bool.cmake-tools"
}